{
  "name": "api-scales",
  "version": "1.0.0",
  "description": "service to get random scales",
  "author": "losWats",
  "license": "ISC",
  "homepage": "https://github.com/cbinput/api-scales#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbinput/api-scales.git"
  },
  "bugs": {
    "url": "https://github.com/cbinput/api-scales/issues"
  },
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "dev-env": "npm run build && node -r dotenv/config dist/server.js",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "test": "jest --coverage=false --verbose",
    "lint": "eslint src/*.ts",
    "lint-fix": "npm run lint --fix",
    "typecheck": "tsc --noEmit",
    "run-prettier": "prettier src/*.ts",
    "validate-prettiness": "npm run run-prettier -c",
    "make-prettier": "npm run run-prettier --write"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.11.1",
    "express": "^4.18.1",
    "morgan": "^1.9.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/morgan": "1.9.3",
    "@types/node": "14.18.21",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.5.1",
    "dotenv": "^16.0.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "26.6.3",
    "jshint": "^2.13.5",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "26.5.6",
    "typescript": "4.7.4"
  }
}
